{
  "etrade": {
    "name": "E*TRADE",
    "type": "investment",
    "keywords": ["etrade", "e*trade", "portfolio", "securities"],
    "patterns": {
      "statement_date": {
        "pattern": "For the Period (\\w+ \\d+-\\d+, \\d{4})",
        "description": "Statement period format"
      },
      "opening_balance": {
        "pattern": "Beginning Total Value \\(as of [^)]+\\) \\$?([\\d,]+\\.?\\d*)",
        "description": "Opening balance at start of period"
      },
      "ending_balance": {
        "pattern": "Ending Total Value \\(as of [^)]+\\) \\$?([\\d,]+\\.?\\d*)",
        "description": "Ending balance at end of period"
      },
      "total_cost_basis": {
        "pattern": "TOTAL VALUE \\d+\\.?\\d*% \\$?([\\d,]+\\.?\\d*) \\$?([\\d,]+\\.?\\d*) \\$?([-\\d,]+\\.?\\d*)",
        "group": 1,
        "description": "Total cost basis from summary section"
      },
      "total_market_value": {
        "pattern": "TOTAL VALUE \\d+\\.?\\d*% \\$?([\\d,]+\\.?\\d*) \\$?([\\d,]+\\.?\\d*) \\$?([-\\d,]+\\.?\\d*)",
        "group": 2,
        "description": "Total market value from summary section"
      },
      "total_unrealized_gain_loss": {
        "pattern": "TOTAL VALUE \\d+\\.?\\d*% \\$?([\\d,]+\\.?\\d*) \\$?([\\d,]+\\.?\\d*) \\$?([-\\d,]+\\.?\\d*)",
        "group": 3,
        "description": "Total unrealized gain/loss from summary section"
      },
      "securities_section": {
        "header_pattern": "Security Description Quantity Share Price Total Cost Market Value Gain/\\(Loss\\) Est Ann Income Yield %",
        "end_patterns": ["TOTAL VALUE", "of Holdings"],
        "description": "Securities section boundaries"
      },
      "security_line": {
        "pattern": "([A-Z\\s&]+)\\s+\\(([A-Z]+)\\)\\s+([\\d,]+\\.?\\d*)\\s+\\$?([\\d,]+\\.?\\d*)\\s+\\$?([\\d,]+\\.?\\d*)\\s+\\$?([\\d,]+\\.?\\d*)\\s+\\$?([-\\d,]+\\.?\\d*)\\s+\\$?([\\d,]+\\.?\\d*)\\s+([\\d,]+\\.?\\d*)",
        "groups": {
          "name": 1,
          "symbol": 2,
          "quantity": 3,
          "share_price": 4,
          "total_cost": 5,
          "market_value": 6,
          "unrealized_gain_loss": 7,
          "income": 8,
          "yield": 9
        },
        "description": "Individual security line format"
      }
    }
  },
