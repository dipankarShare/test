{
  "chase_credit": {
    "name": "Chase Credit Card",
    "type": "credit_card",
    "keywords": [
      "chase",
      "credit card",
      "chase freedom",
      "chase sapphire",
      "chase slate"
    ],
    "patterns": {
      "statement_date": {
        "pattern": "(?:Statement Date|Closing Date|Date)[:\\s]*(\\d{1,2}[/-]\\d{1,2}[/-]\\d{2,4})",
        "description": "Credit card statement date"
      },
      "due_date": {
        "pattern": "(?:Payment Due Date|Due Date|Payment Due)[:\\s]*(\\d{1,2}[/-]\\d{1,2}[/-]\\d{2,4})",
        "description": "Payment due date"
      },
      "opening_balance": {
        "pattern": "(?:Previous Balance|Previous Statement Balance|Beginning Balance)[:\\s]*\\$?([-\\d,]+\\.?\\d*)",
        "description": "Previous statement balance"
      },
      "ending_balance": {
        "pattern": "(?:New Balance|Current Balance|Statement Balance|Total Amount Due)[:\\s]*\\$?([-\\d,]+\\.?\\d*)",
        "description": "Current statement balance"
      },
      "minimum_payment": {
        "pattern": "(?:Minimum Payment|Minimum Payment Due)[:\\s]*\\$?([-\\d,]+\\.?\\d*)",
        "description": "Minimum payment amount"
      },
      "credit_limit": {
        "pattern": "(?:Credit Limit|Available Credit|Credit Line)[:\\s]*\\$?([-\\d,]+\\.?\\d*)",
        "description": "Credit limit amount"
      },
      "available_credit": {
        "pattern": "(?:Available Credit|Available Amount)[:\\s]*\\$?([-\\d,]+\\.?\\d*)",
        "description": "Available credit amount"
      },
      "transactions_section": {
        "header_pattern": "Date|Description|Amount|Category|Reference",
        "end_patterns": [
          "Total",
          "Summary",
          "Payment Information",
          "Fees and Interest"
        ],
        "description": "Credit card transactions section"
      },
      "transaction_line": {
        "pattern": "(\\d{1,2}[/-]\\d{1,2}[/-]\\d{2,4})\\s+([^\\$]+?)\\s+\\$?([-\\d,]+\\.?\\d*)",
        "groups": {
          "date": 1,
          "description": 2,
          "amount": 3
        },
        "description": "Individual transaction line format"
      },
      "fees_section": {
        "header_pattern": "Fees|Interest|Charges",
        "end_patterns": ["Total", "Summary"],
        "description": "Fees and interest charges section"
      }
    }
  },
